/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1


 sel  a   b   
  0   in  0   
  1   0   in   
  
 */

CHIP DMux2 {
    IN in, sel;
    OUT a, b;

    PARTS:
    Not(in=sel, out=notSel);
    Nand(a=notSel, b=notSel, out=a);
    Nand(a=in, b=notSel, out=b);
}
